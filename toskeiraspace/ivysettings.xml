<ivysettings>
  <settings defaultResolver="default"/>
  <resolvers>

    <ibiblio name="spark-snapshots"
               m2compatible="true"
               usepoms="true"
               pattern="[organization]/[module]/[revision]/spark-0.9.9.3-20110901.075332-1.[ext]"
               root="http://46.137.105.19:8081/nexus/content/repositories/spark"/>

    <ibiblio name="repo1" root="http://repo1.maven.org/maven/2/" pattern="[organization]/jars/[module]-[revision].[ext]" />

    <chain name="default" changingPattern=".*SNAPSHOT" checkmodified="true">

      <resolver ref="repo1"/>
      <resolver ref="spark-snapshots"/>

    </chain>
  </resolvers>
</ivysettings>
